{
  "name": "@pct-org/scraper",
  "version": "3.0.0",
  "description": "Scraper part of PCT environment",
  "license": "MIT",
  "files": [
    "./build/**"
  ],
  "main": "./build/scraper.js",
  "module": "./build/scraper.esm.js",
  "bin": {
    "scraper": "./build/scraper.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pct-org/scraper.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "docs": "esdoc -c .esdoc.json",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "forever": "cross-env NODE_ENV=production forever start ./build/popcorn-api.js",
    "lint": "eslint {src,test}/** ./*.js",
    "providers": "babel-node ./src/index.js --providers",
    "prepushold": "npm run test",
    "prerelease": "npm run build && npm run docs",
    "release": "git add ./docs/* && standard-version -a",
    "start": "cross-env NODE_ENV=production node ./build/popcorn-api.js",
    "start:debug": "yarn start:dev --inspect-brk",
    "start:dev": "cross-env NODE_ENV=development cross-env DEBUG=@pct-org/* babel-node ./src/index.js -m pretty",
    "start:dev:crawl": "yarn start:dev -- -s",
    "pretest": "npm run build",
    "test": "npm run test-dredd && npm run test-mocha",
    "test-dredd": "cross-env NODE_ENV=test babel-node ./node_modules/dredd/bin/dredd",
    "test-mocha": "cross-env NODE_ENV=test nyc -r=lcov -r=text mocha --exit --recursive --timeout 10000"
  },
  "dependencies": {
    "@pct-org/pop-api": "0.6.3",
    "bytes": "^3.0.0",
    "dotenv": "8.1.0",
    "eztv-api-pt": "^1.3.1",
    "fanart.tv-api": "^2.0.1",
    "gulp": "4.0.2",
    "horriblesubs-api": "^1.0.8",
    "inquirer": "7.0.0",
    "kat-api-pt": "^4.0.0",
    "mongoose": "^5.0.1",
    "node-tvdb": "4.1.0",
    "nyaa-api-pt": "^2.0.0",
    "omdb-api-pt": "^2.0.1",
    "p-map": "3.0.0",
    "p-times": "2.1.0",
    "parse-torrent": "7.0.1",
    "pop-api-scraper": "^0.1.2",
    "tmdbapi": "^1.1.0",
    "trakt.tv": "7.2.0",
    "webtorrent-health": "^1.1.2",
    "yts-api-pt": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "5.2.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "del": "5.1.0",
    "dredd": "12.0.7",
    "esdoc": "^1.0.4",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-flow-type-plugin": "1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "6.3.0",
    "eslint-config-vixo": "^2.0.2",
    "express": "^4.16.2",
    "flow": "^0.2.3",
    "flow-bin": "0.107.0",
    "flow-typed": "^2.2.3",
    "forever": "1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "rollup": "1.21.2",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-uglify": "6.0.3",
    "sinon": "7.4.2",
    "standard-version": "7.0.0",
    "supertest": "4.0.2",
    "uglify-es": "^3.3.4"
  },
  "engines": {
    "node": ">=7.10.1"
  }
}
